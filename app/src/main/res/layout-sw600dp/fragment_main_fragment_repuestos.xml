            android:layout_height="match_parent" />

        <fragment
            android:id="@+id/f_detalle_repuesto"
            android:name="com.electivaIII.sistemainventario.fragments.ChangeDataFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>

</FrameLayout>