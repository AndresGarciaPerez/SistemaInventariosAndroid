    android:layout_height="match_parent"
    tools:context=".fragments.FragContenedores.MainFragmentRepuestos">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <fragment
            android:id="@+id/frMainRepuestos"
            android:name="com.electivaIII.sistemainventario.fragments.RepuestosFile.ListRepuestos"
